{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "yes:explosion_smoke",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/smoke"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.width = 1;variable.height = 1;variable.size_mult = 1;variable.random_direction_x = math.random(-1, 1);variable.random_direction_z = math.random(-1, 1);",
				"per_update_expression": "variable.size_mult = v.emitter_age < 1 ? (math.pow(v.emitter_age, 1.4) + 1) : (0.56*math.ln(v.emitter_age-0.6) + 2-(0.56*math.ln(0.4)));"
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": 100
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 8
			},
			"minecraft:emitter_shape_point": {
				"direction": ["math.random(-1,1)", "math.random(-0.5,0.5)", "math.random(-1,1)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "4+ Math.Random(0, 2)"
			},
			"minecraft:particle_initial_speed": "math.random(1,10)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["v.particle_random_2*5*v.random_direction_x", 2, "v.particle_random_3*5*v.random_direction_z"],
				"linear_drag_coefficient": "math.min(math.pow(v.particle_age*5, 2), 2)"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.width*variable.size_mult", "variable.height*variable.size_mult"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 32,
					"texture_height": 32,
					"uv": [0, 0],
					"uv_size": [32, 32]
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.08": "#2BBDBDBD",
						"0.54": "#08B4B4B4",
						"1.0": "#00A2A2A2"
					}
				}
			}
		}
	}
}