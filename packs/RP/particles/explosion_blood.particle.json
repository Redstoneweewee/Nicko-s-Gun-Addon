{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "yes:explosion_blood",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/blood2"
			}
		},
		"events": {
			"hit_ground": {
				"expression": "variable.width = variable.width+math.random(0.0015/v.amount*35, 0.0045/v.amount*35);variable.height = variable.height < 0.012 ? 0.012 : variable.height+math.random(-0.001/v.amount*35, 0);"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.width = 0.15;variable.height = 0.1;variable.spread_deg = 5;"
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": "v.amount"
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.1
			},
			"minecraft:emitter_shape_point": {
				"direction": ["math.random(-0.6,0.6)", "math.random(0,0.3)", "math.random(-0.6,0.6)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "25 + Math.Random(1, 16)",
				"expiration_expression": "math.random_integer(0, 10) == 0 ? query.total_particle_count > 10000 : 0"
			},
			"minecraft:particle_initial_spin": {
				"rotation": 90
			},
			"minecraft:particle_initial_speed": "math.random(2,20)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0, -9.8, 0],
				"linear_drag_coefficient": 1
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.width*variable.particle_random_1", "variable.height*variable.particle_random_1"],
				"facing_camera_mode": "direction_y",
				"uv": {
					"texture_width": 32,
					"texture_height": 32,
					"uv": [0, 0],
					"uv_size": [32, 32]
				}
			},
			"minecraft:particle_motion_collision": {
				"collision_drag": 5,
				"collision_radius": 0.01,
				"events": [
					{
						"event": "hit_ground",
						"min_speed": 0
					}
				]
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#ffffffff",
						"0.41": "#42FFFFFF",
						"0.63": "#36FFFFFF",
						"1.0": "#00FFFFFF"
					}
				}
			}
		}
	}
}